{
  "presets": ["env", "es2015", "react"],
  "env": {
    "development": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-react-jsx-source"
      ]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./build/index"],
      "alias": {
        "@apis/*": ["./build/apis/"],
        "@css/*": ["./css/"],
        "@pages/*": ["./build/components/pages/"],
        "@navigations/*": ["./build/components/navigations/"],
        "@models/*": ["./build/models/"],
        "@stores/*": ["./build/stores/"],
        "@utils/*": ["./build/utils/"],
        "@assets/*": ["./assets/"],
        "@constants/*" : ["./build/constants/"],
        "@shared/*": ["./build/components/shared/"],
        "@App_dev": ["./App_dev"],
        "@app": ["./build/app"]
      }
    }]
  ]
}
