{
  "name": "react-webpack3-starter",
  "version": "1.0.0",
  "description": "starting react express app with me!",
  "postinstall": "tsc",
  "scripts": {
    "dev": "webpack-dev-server",
    "start": "webpack-dashboard -- webpack-dev-server -d --hot --config webpack.config.js --watch",
    "build": "rm -rf ./build && tsc",
    "test": "rm -rf ./build && tsc && jest -u",
    "prod": "webpack -p",
    "production": "NODE_ENV=production webpack -p"
  },
  "jest": {
    "setupFiles": [
      "./test/shimSetup.js",
      "./test/jestSetup.js"
    ],
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer"
    ]
  },
  "author": "hyochan",
  "license": "MIT",
  "dependencies": {
    "mobx": "^3.3.2",
    "mobx-react": "^4.3.5",
    "react": "^16.1.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@types/css": "0.0.30",
    "@types/inline-style-prefixer": "^3.0.1",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.13",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux": "^3.6.0",
    "autobind-decorator": "^2.1.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "expect": "^21.2.1",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "jest": "^21.2.1",
    "node-sass": "^4.7.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "radium": "^0.19.6",
    "react-test-renderer": "^16.2.0",
    "script-loader": "^0.7.2",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.2.0",
    "tslib": "^1.8.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
